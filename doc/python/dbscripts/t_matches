CREATE TABLE `t_matches` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `host_team` varchar(45) DEFAULT NULL,
  `guest_team` varchar(45) DEFAULT NULL,
  `host_win` float DEFAULT NULL,
  `host_lost` float DEFAULT NULL,
  `host_deduce` float DEFAULT NULL,
  `host_half_win` float DEFAULT NULL,
  `host_half_lost` float DEFAULT NULL,
  `host_half_deduce` float DEFAULT NULL,
  `match_odds` varchar(45) DEFAULT NULL,
  `match_host_odds` float DEFAULT NULL,
  `match_guest_odds` float DEFAULT NULL,
  `match_big` varchar(45) DEFAULT NULL,
  `match_host_big` float DEFAULT NULL,
  `match_guest_small` float DEFAULT NULL,
  `match_half_odds` varchar(45) DEFAULT NULL,
  `match_half_host_odds` float DEFAULT NULL,
  `match_half_guest_odds` float DEFAULT NULL,
  `match_half_big` varchar(45) DEFAULT NULL,
  `match_half_host_big` float DEFAULT NULL,
  `match_half_guest_small` float DEFAULT NULL,
  `create_date` timestamp NULL DEFAULT NULL,
  `match_result` varchar(45) DEFAULT NULL,
  `match_time` varchar(45) DEFAULT NULL,
  `is_goal` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=816 DEFAULT CHARSET=utf8;
